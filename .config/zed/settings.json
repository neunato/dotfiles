{
   "theme": "Catppuccin Macchiato",
   "base_keymap": "None",
   "features": {
      "edit_prediction_provider": "supermaven"
   },
   "buffer_font_family": "JetBrains Mono",
   "buffer_font_size": 17,
   "buffer_font_weight": 400,
   "ui_font_family": "JetBrains Mono",
   "ui_font_size": 17,
   "ui_font_weight": 500,
   "agent_buffer_font_size": 15,
   "unnecessary_code_fade": 0.5,
   "image_viewer": {
      "unit": "decimal"
   },
   "hover_popover_delay": 100,
   "when_closing_with_no_tabs": "keep_window_open",
   "hide_mouse": "never",
   "lsp_highlight_debounce": 50,
   "minimum_contrast_for_highlights": 50,
   "auto_signature_help": true,
   "drag_and_drop_selection": {
      "enabled": false
   },
   "redact_private_values": true,
   "expand_excerpt_lines": 10,
   "excerpt_context_lines": 3,
   "use_on_type_format": false,
   "auto_indent": false,
   "auto_indent_on_paste": true,
   "always_treat_brackets_as_autoclosed": true,
   "show_edit_predictions": true,
   "toolbar": {
      "quick_actions": false,
      "selections_menu": false,
      "code_actions": true
   },
   "title_bar": {
      "show_branch_icon": false,
      "show_branch_name": false,
      "show_project_items": true,
      "show_onboarding_banner": false,
      "show_user_picture": false,
      "show_sign_in": false,
      "show_menus": false
   },
   "scrollbar": {
      "search_results": false,
      "selected_symbol": false,
      "diagnostics": null
   },
   "minimap": {
      "show": "never",
      "display_in": "all_editors",
      "thumb_border": "none",
      "current_line_highlight": "none"
   },
   "middle_click_paste": false,
   "gutter": {
      "runnables": false,
      "breakpoints": false
   },
   "scroll_beyond_last_line": "off",
   "vertical_scroll_margin": 10,
   "autoscroll_on_clicks": false,
   "horizontal_scroll_margin": 5,
   "scroll_sensitivity": 1.5,
   "fast_scroll_sensitivity": 15.0,
   "search": {
      "button": false
   },
   "resize_all_panels_in_dock": ["left", "right", "bottom"],
   "project_panel": {
      "default_width": 300,
      "dock": "left",
      "entry_spacing": "standard",
      "indent_size": 21,
      "auto_reveal_entries": false,
      "auto_fold_dirs": false,
      "hide_root": true
   },
   "outline_panel": {
      "default_width": 300,
      "dock": "right",
      "indent_size": 18,
      "indent_guides": {
         "show": "never"
      }
   },
   "collaboration_panel": {
      "button": false
   },
   "git_panel": {
      "button": false
   },
   "message_editor": {
      "auto_replace_emoji_shortcode": false
   },
   "notification_panel": {
      "button": false
   },
   "agent": {
      "enabled": false
   },
   "autosave": "off",
   "tab_bar": {
      "show_nav_history_buttons": false,
      "show_tab_bar_buttons": false
   },
   "tabs": {
      "git_status": true
   },
   "preview_tabs": {
      "enable_preview_from_code_navigation": true
   },
   "extend_comment_on_newline": false,
   "format_on_save": "off",
   "soft_wrap": "none",
   "preferred_line_length": 10000,
   "tab_size": 3,
   "telemetry": {
      "diagnostics": false,
      "metrics": false
   },
   "auto_update": false,
   "git": {
      "inline_blame": {
         "enabled": false
      },
      "branch_picker": {
         "show_author_name": false
      },
      "hunk_style": "unstaged_hollow"
   },
   "status_bar": {
      "experimental.show": false
   },
   "terminal": {
      "button": false
   },
   "auto_install_extensions": {
      "catppuccin": true,
      "deno": true,
      "dockerfile": true,
      "git-firefly": true,
      "html": true,
      "kdl": true,
      "lua": true,
      "make": true,
      "scss": true,
      "svelte": true,
      "toml": true,
      "vue": true
   },
   "completions": {
      "words": "enabled",
      "words_min_length": 2
   },
   "jupyter": {
      "enabled": false
   },
   "read_ssh_config": false,
   "debugger": {
      "button": false
   }
}

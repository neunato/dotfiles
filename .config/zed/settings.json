{
   "theme": {
      "mode": "dark",
      "light": "Solarized Light",
      "dark": "Ayu Dark"
   },
   "base_keymap": "SublimeText",
   "features": {
      "copilot": false,
      "inline_completion_provider": "none"
   },
   "buffer_font_family": "JetBrains Mono",
   "buffer_font_features": {
      "calt": true
   },
   "buffer_font_size": 17,
   "buffer_font_weight": 400,
   "buffer_line_height": "comfortable",
   "ui_font_family": "JetBrains Mono",
   "ui_font_features": {
      "calt": true
   },
   "ui_font_weight": 600,
   "ui_font_size": 18,
   "active_pane_magnification": 1.0,
   "centered_layout": {
      "left_padding": 0.2,
      "right_padding": 0.2
   },
   "multi_cursor_modifier": "alt",
   "vim_mode": false,
   "hover_popover_enabled": true,
   "confirm_quit": false,
   "restore_on_startup": "last_workspace",
   "drop_target_size": 0.15,
   "when_closing_with_no_tabs": "keep_window_open",
   "cursor_blink": true,
   "current_line_highlight": "all",
   "show_completions_on_input": true,
   "show_completion_documentation": true,
   "completion_documentation_secondary_query_debounce": 100,
   "show_wrap_guides": true,
   "wrap_guides": [],
   "redact_private_values": false,
   "expand_excerpt_lines": 10,
   "private_files": [
      "**/.env*",
      "**/*.pem",
      "**/*.key",
      "**/*.cert",
      "**/*.crt",
      "**/secrets.yml"
   ],
   "use_on_type_format": true,
   "use_autoclose": true,
   "use_auto_surround": true,
   "always_treat_brackets_as_autoclosed": true,
   "show_inline_completions": true,
   "show_whitespaces": "selection",
   "calls": {
      "mute_on_join": true,
      "share_on_join": false
   },
   "toolbar": {
      "breadcrumbs": true,
      "quick_actions": false,
      "selections_menu": false
   },
   "scrollbar": {
      "show": "auto",
      "cursors": true,
      "git_diff": true,
      "search_results": true,
      "selected_symbol": true,
      "diagnostics": true
   },
   "double_click_in_multibuffer": "select",
   "gutter": {
      "line_numbers": true,
      "code_actions": true,
      "runnables": true,
      "folds": true
   },
   "indent_guides": {
      "enabled": false,
      "line_width": 1,
      "active_line_width": 1,
      "coloring": "fixed",
      "background_coloring": "disabled"
   },
   "scroll_beyond_last_line": "vertical_scroll_margin",
   "vertical_scroll_margin": 5,
   "scroll_sensitivity": 1.5,
   "relative_line_numbers": false,
   "seed_search_query_from_cursor": "always",
   "inlay_hints": {
      "enabled": true,
      "show_type_hints": true,
      "show_parameter_hints": true,
      "show_other_hints": true,
      "edit_debounce_ms": 400,
      "scroll_debounce_ms": 30
   },
   "project_panel": {
      "button": true,
      "default_width": 300,
      "dock": "left",
      "file_icons": true,
      "folder_icons": true,
      "git_status": true,
      "indent_size": 15,
      "auto_reveal_entries": true,
      "auto_fold_dirs": false,
      "scrollbar": {
         "show": "never"
      }
   },
   "outline_panel": {
      "button": true,
      "default_width": 300,
      "dock": "right",
      "file_icons": true,
      "folder_icons": true,
      "git_status": true,
      "indent_size": 15,
      "auto_reveal_entries": true,
      "auto_fold_dirs": true
   },
   "collaboration_panel": {
      "button": false,
      "dock": "right",
      "default_width": 300
   },
   "chat_panel": {
      "button": false,
      "dock": "right",
      "default_width": 300
   },
   "message_editor": {
      "auto_replace_emoji_shortcode": false
   },
   "notification_panel": {
      "button": false,
      "dock": "right",
      "default_width": 300
   },
   "assistant": {
      "version": "1",
      "enabled": false,
      "button": false,
      "dock": "right",
      "default_width": 600,
      "default_height": 400,
      "provider": {
         "name": "openai",
         "default_model": "gpt-4o"
      }
   },
   "show_call_status_icon": true,
   "enable_language_server": true,
   "linked_edits": true,
   "language_servers": [
      "..."
   ],
   "autosave": "off",
   "tab_bar": {
      "show": true,
      "show_nav_history_buttons": false
   },
   "tabs": {
      "git_status": true,
      "close_position": "right"
   },
   "preview_tabs": {
      "enabled": true,
      "enable_preview_from_file_finder": false,
      "enable_preview_from_code_navigation": false
   },
   "remove_trailing_whitespace_on_save": true,
   "extend_comment_on_newline": true,
   "ensure_final_newline_on_save": true,
   "format_on_save": "off",
   "formatter": "auto",
   "soft_wrap": "editor_width",
   "preferred_line_length": 80,
   "hard_tabs": false,
   "tab_size": 3,
   "telemetry": {
      "diagnostics": true,
      "metrics": false
   },
   "auto_update": false,
   "diagnostics": {
      "include_warnings": true
   },
   "file_scan_exclusions": [
      "**/.git",
      "**/.svn",
      "**/.hg",
      "**/CVS",
      "**/.DS_Store",
      "**/Thumbs.db",
      "**/.classpath",
      "**/.settings"
   ],
   "git": {
      "git_gutter": "tracked_files",
      "gutter_debounce": 0,
      "inline_blame": {
         "enabled": false,
         "delay_ms": 0,
         "min_column": 120
      }
   },
   "inline_completions": {
      "disabled_globs": [
         ".env"
      ]
   },
   "journal": {
      "path": "~/.config/zed",
      "hour_format": "hour24"
   },
   "terminal": {
      "shell": "system",
      "dock": "bottom",
      "default_width": 600,
      "default_height": 400,
      "working_directory": "current_project_directory",
      "blinking": "on",
      "alternate_scroll": "off",
      "option_as_meta": false,
      "copy_on_select": false,
      "button": true,
      "env": {},
      "line_height": "standard",
      "detect_venv": {
         "on": {
            "directories": [
               ".env",
               "env",
               ".venv",
               "venv"
            ],
            "activate_script": "default"
         }
      },
      "toolbar": {
         "title": true
      },
      "font_size": 16,
      // "font_family": "Zed Plex Mono",
      "max_scroll_history_lines": 50000
   },
   "code_actions_on_format": {},
   "tasks": {
      "variables": {}
   },
   "file_types": {
      "JSON": [
         "flake.lock"
      ],
      "JSONC": [
         "**/.zed/**/*.json",
         "**/zed/**/*.json"
      ]
   },
   "auto_install_extensions": {
      "html": true
   },
   "languages": {},
   "prettier": {},
   "lsp": {},
   "vim": {},
   // "server_url": "https://zed.dev",
   "preview": {},
   "nightly": {},
   "stable": {},
   "dev": {},
   "task": {
      "show_status_indicator": true
   },
   "line_indicator_format": "long",
   "proxy": null
}
